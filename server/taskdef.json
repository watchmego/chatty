{
    "executionRoleArn": "arn:aws:iam::997388814125:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "socketIO-server",
            "image": <Image1_NAME>,
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 8000,
